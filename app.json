{
    "name": "Heroku manager bot",
    "description": "Powerful Telegram Heroku manager bot.",
    "repository": "https://github.com/Doctorstra/heroku-manager",
    "keywords": [
        "Heroku manager bot",
        
    ],
    "env": {
        "API_ID": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "A valid Pyrogram session name",
            "required": true
        },
        "HEROKU_API_KEY": {
            "description": "Your time zone (Example: Asia/Kolkata)",
            "required": true
        },
        "AUTHORIZED_USERS": {
        "description": "for user in os.environ.get",
        "required": true
        },
        "HEROKU_APP_NAME": {
        "description": "for",
        "required": true
        },
        "TG_CHARACTER_LIMIT":{
        "description": "hello",
        "required": true
        }
    },
    "formation": {
    "worker": {                
      "quantity": 1,
      "size": "free"
    }
  },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
